components:
  securitySchemes:
    bearer:
      scheme: bearer
      bearerFormat: JWT
      type: http
  schemas:
    UserEntity:
      type: object
      properties:
        id:
          type: number
          description: 유저 PK
          example: 1
        name:
          type: string
          description: 유저이름
          example: 최은강
        account:
          type: string
          description: 유저계정
          examples:
            kakao: k-{kakao-id}
            google: g-{google-id}
            apple: a-{apple-id}
        intro:
          type: string
          description: 유저 소개글
          example: Trazzle로 세계정복!
        profileImageURL:
          type: string
          description: 유저 프로필 이미지
