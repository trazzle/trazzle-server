          content:
            application/json:
              schema:
                $ref: '#/components/schemas/SignInOrSignUpAppleRequestBodyDto'

  /api/users/sign-in/google:
    post:
      operationId: UsersController_signInGoogle
      summary: 구글 연동 로그인
      parameters: []
      requestBody:
        required: true
        content:
          application/json:
